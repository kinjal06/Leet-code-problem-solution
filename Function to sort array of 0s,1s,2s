#include<stdio.h>
#include<stdlib.h>

void sort(int a[],int len)
{
	int i=0;
	int mid = 0;
	int high = len-1;
	while(mid<=high){
		switch(a[mid]){
			case 0:
				swap(&a[i++],a[mid++])
				break;
			case 1:
				mid++;
				break,
			case 2:
				swap(&a[mid],&a[high--]);
				break;
				
		}
	}
}

void swap( int *x, int *y){
	int temp = *x;
	*x =*y;
	*y = temp;
}
